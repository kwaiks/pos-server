{
  "name": "mesan-api",
  "version": "1.0.0",
  "description": "API for Mesan",
  "main": "server.js",
  "author": "Alexander Jacquelline",
  "license": "MIT",
  "scripts": {
    "migrate": "knex migrate:make -x ts $1 --knexfile ./src/knexConfig.ts",
    "seed": "knex seed:make -x ts $1 --knexfile ./src/knexConfig.ts",
    "migrate:latest": "knex migrate:latest  --knexfile ./src/knexConfig.ts",
    "seed:run": "knex seed:run  --knexfile ./src/knexConfig.ts",
    "start": "ts-node -r tsconfig-paths/register -r ts-node ./src/server.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "nodemon --config nodemon.json src/server.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/server.ts",
    "test": "NODE_ENV=test mocha --exit -r ts-node/register --require tsconfig-paths/register 'test/**/*.ts' "
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-useragent": "^1.0.0",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.16.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "express-validator": "^6.8.0",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "objection": "^2.2.7",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "winston": "^3.3.3"
  },
  "_moduleAliases": {
    "@/controllers/*": "dist/controllers/*",
    "@/services/*": "dist/services/*",
    "@/loaders/*": "dist/loaders/*",
    "@/models/*": "dist/models/*",
    "@/config/*": "dist/config/*"
  }
}
